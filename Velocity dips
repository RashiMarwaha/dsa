#include <bits/stdc++.h>
using namespace std;
int main()
{
    int in_vel;
    cout<<"Enter initial velocity: ";
    cin>>in_vel;
    int count=0;
    while(in_vel!=0)
    {
        in_vel=in_vel/2;
        count++;
    }
    cout<<"Number of dips before velocity becomes zero is: "<<count;




    return 0;
}
